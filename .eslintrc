{
  'extends': 'eslint:recommended',
  'env': {
    'node': true,
    'mocha': true,
    'jasmine': true
  },
  'rules': {
    'no-console': 0,
    'array-callback-return': 2,
    'curly': 1,
    'dot-notation': 1,
    'eqeqeq': 2,
    'no-alert': 2,
    'no-eval': 1,
    'no-implied-eval': 1,
    'no-invalid-this': 2,
    'no-loop-func': 1,
    'no-return-assign': 2,
    'no-self-compare': 2,
    'global-require': 2,
    'handle-callback-err': 1,
    'no-mixed-requires': 2,
    'camelcase': [2, {'properties': 'always'}],
    'eol-last': 1,
    'indent': [2, 2],
    'no-nested-ternary': 2,
    'no-ternary': 2,
    'one-var-declaration-per-line': [2, 'always']
  }
}
